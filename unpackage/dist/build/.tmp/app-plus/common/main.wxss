/* 单行溢出隐藏 */
.oneline {
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
/* 两行移除隐藏 */
.twoline {
	white-space: normal;
	word-break: break-all;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
}
/* 三行移除隐藏 */
.threeline {
	white-space: normal;
	word-break: break-all;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 3;
}
